<h1>Listing UserVotes</h1>

<table>
  <thead>
    <tr>
      <th>Vote status</th>
      <th>User</th>
      <th>Slam</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_votes.each do |user_vote| %>
      <tr>
        <td><%= user_vote.vote_status %></td>
        <td><%= user_vote.user_id %></td>
        <td><%= user_vote.slam_id %></td>
        <td><%= link_to 'Show', user_vote %></td>
        <td><%#= link_to 'Edit', edit_user_vote_path(user_vote) %></td>
        <td><%#= link_to 'Destroy', user_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

